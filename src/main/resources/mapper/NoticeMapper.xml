<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.mapper.NoticeMapper">
	<select id="getNoticeList" resultType="com.project.domain.Notice">
		select * from (
			select notice.* , ROWNUM AS rn 
			from notice 
			where ROWNUM &lt; #{endRow}   <!-- <= 부분을 &lt;= 로 변경 -->
			)
		 where rn &gt;= #{startRow} <!-- > 부분을 &gt; 로 변경 -->
	</select>
	
	<select id="getEventList" resultType="com.project.domain.Event">
		select * from (
			select event.* , ROWNUM AS rn 
			from event 
			where ROWNUM &lt; #{endRow}   <!-- <= 부분을 &lt;= 로 변경 -->
			)
		 where rn &gt;= #{startRow} <!-- > 부분을 &gt; 로 변경 -->
	</select>
	
	<select id="getNoticeCountList" resultType="int">
		select count(*) from notice
	</select>
	
	<select id="getEventCountList" resultType="int">
		select count(*) from event
	</select>
	
</mapper>